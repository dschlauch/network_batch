\begin_body

\begin_layout Standard
\begin_inset Formula 
\[
E_{h}=\sum_{i=1}^{N}\left(\mathbf{G}_{i}\mathbf{G}_{i}^{T}-\mathbf{Q}_{h}\left[\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\right]\mathbf{Q}_{h}^{T}\right)^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E_{h} & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}-\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)^{2}\\
 & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)^{2}-2\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}+\left(\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\right)^{2}\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)^{2}\right)\\
\frac{\partial}{\partial\gamma_{\cdot h}}E_{h} & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(-2\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\mathbf{X}_{i,\cdot}^{T}+2\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)^{2}\left(\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}^{T}\right)\right)\\
0 & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(-2\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\mathbf{X}_{i,\cdot}^{T}\right)+\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(2\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)^{2}\left(\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}^{T}\right)\right)\\
\left(\sum_{j=1,k>j}^{p}\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)^{2}\right)^{-1}\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\mathbf{X}_{i,\cdot}^{T} & =\sum_{i=1}^{N}\left(\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}^{T}\right)
\end{align*}

\end_inset


\end_layout

