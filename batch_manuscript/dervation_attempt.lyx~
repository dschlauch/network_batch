\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mathbf{q}^{T}\mathbf{X}\left(\mathbf{G}-\mathbf{q}\hat{\Psi}^{T}\mathbf{X}^{T}\right) & =0\\
\mathbf{q}^{T}\hat{\Psi}^{T}\mathbf{X}^{T} & =\mathbf{q}^{T}\mathbf{G}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
zxc
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mathbf{0}_{q} & =\sum_{i=1}^{N}\mathbf{X}_{i}^{T}\left[\mathbf{Q}_{h}^{T}\left[\mathbf{G}_{i}\mathbf{G}_{i}^{T}-\mathbf{Q}_{h}\mathbf{X}_{i}\hat{\Psi}_{h}\mathbf{Q}_{h}^{T}\right]\mathbf{Q}_{h}\right]\\
\mathbf{0}_{q} & =\sum_{i=1}^{N}\left[\mathbf{X}_{i}^{T}\mathbf{Q}_{h}^{T}\mathbf{G}_{i}\mathbf{G}_{i}^{T}\mathbf{Q}_{h}-\mathbf{X}_{i}^{T}\mathbf{Q}_{h}^{T}\mathbf{Q}_{h}\mathbf{X}_{i}\hat{\Psi}_{h}\mathbf{Q}_{h}^{T}\mathbf{Q}_{h}\right]\\
\mathbf{0}_{q} & =\sum_{i=1}^{N}\left[\mathbf{X}_{i}^{T}\mathbf{Q}_{h}^{T}\mathbf{G}_{i}\mathbf{G}_{i}^{T}\mathbf{Q}_{h}-\mathbf{X}_{i}^{T}\mathbf{X}_{i}\hat{\Psi}_{h}\right]\\
\sum_{i=1}^{N}\left[\mathbf{X}_{i}^{T}\mathbf{X}_{i}\right]\hat{\Psi}_{h} & =\sum_{i=1}^{N}\left[\mathbf{X}_{i}^{T}\mathbf{Q}_{h}^{T}\mathbf{G}_{i}\mathbf{G}_{i}^{T}\mathbf{Q}_{h}\right]\\
\mathbf{X}^{T}\mathbf{X}\hat{\Psi}_{h} & =\sum_{i=1}^{N}\left[\mathbf{X}_{i}^{T}\mathbf{Q}_{h}^{T}\mathbf{G}_{i}\mathbf{G}_{i}^{T}\mathbf{Q}_{h}\right]\\
\hat{\Psi}_{h} & =\left(\mathbf{X}^{T}\mathbf{X}\right)^{-1}\sum_{i=1}^{N}\left[\mathbf{X}_{i}^{T}\mathbf{Q}_{h}^{T}\mathbf{G}_{i}\mathbf{G}_{i}^{T}\mathbf{Q}_{h}\right]\\
\\
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\mathbf{Y}$
\end_inset

 be a vector of matrices, where 
\begin_inset Formula $\mathbf{Y}_{i}=\mathbf{G}_{i}\mathbf{G}_{i}^{T}$
\end_inset

 and let 
\begin_inset Formula $\mathbf{D}=f\left(\mathbf{X}\hat{\Psi}\right)$
\end_inset

 be a 4D matrix with dimensions 
\begin_inset Formula $p\times p\times N\times q$
\end_inset

 where 
\begin_inset Formula $\mathbf{X}\hat{\Psi}$
\end_inset

 is a 
\begin_inset Formula $p\times N$
\end_inset

 matrix on the diagonal of 
\begin_inset Formula $\mathbf{D}$
\end_inset

.
\begin_inset Formula 
\begin{align*}
0 & =\left[\mathbf{Q}\mathbf{X}\mathbf{Q}^{T}\right]^{T}\left(\mathbf{Y}-\left[\mathbf{Q}\left[\mathbf{D}\right]\mathbf{Q}^{T}\right]\right)\\
0 & =\left[\mathbf{Q}\mathbf{X}\mathbf{Q}^{T}\right]^{T}\mathbf{Q}\mathbf{Y}\mathbf{Q}^{T}-\left[\mathbf{Q}\mathbf{X}\mathbf{Q}^{T}\right]^{T}\mathbf{Q}\left[\mathbf{X}\hat{\Psi}\right]\mathbf{Q}^{T}\\
0 & =\mathbf{Q}\mathbf{X}^{T}\mathbf{Q}^{T}\mathbf{Y}-\mathbf{Q}\mathbf{X}^{T}\mathbf{Q}^{T}\mathbf{Q}\left[\mathbf{X}\hat{\Psi}\right]\mathbf{Q}^{T}\\
\mathbf{Q}\mathbf{X}^{T}\mathbf{Q}^{T}\mathbf{Q}\left[\mathbf{X}\hat{\Psi}\right]\mathbf{Q}^{T} & =\mathbf{Q}\mathbf{X}^{T}\mathbf{Q}^{T}\mathbf{Q}\mathbf{Y}\mathbf{Q}^{T}\\
\mathbf{Q}\mathbf{X}^{T}\left[\mathbf{X}\hat{\Psi}\right]\mathbf{Q}^{T} & =\mathbf{Q}\mathbf{X}^{T}\mathbf{Y}\mathbf{Q}^{T}\\
\mathbf{X}^{T}\mathbf{X}\hat{\Psi}\mathbf{Q}^{T} & =\mathbf{X}^{T}\mathbf{Q}^{T}\mathbf{Y}\\
\hat{\Psi} & =\left(\mathbf{X}^{T}\mathbf{X}\right)^{-1}\mathbf{X}^{T}\mathbf{Q}^{T}\mathbf{Y}\mathbf{Q}^{-T}\\
\\
\mathbf{Q}_{h}^{T}\left[\mathbf{X}\gamma_{\cdot,h}\right]^{T}\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\mathbf{X}^{T}\mathbf{Q}_{h} & =Y\mathbf{Q}_{h}^{T}\mathbf{X}^{T}\mathbf{Q}_{h}\\
\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\gamma_{\cdot,h}^{T}\mathbf{X}\mathbf{X}^{T} & =Y\mathbf{X}^{T}\\
\gamma_{\cdot,h}^{T} & =Y\mathbf{X}^{T}\left(\mathbf{X}\mathbf{X}^{T}\right)^{-1}\left(\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\right)^{-1}\\
\gamma_{\cdot,h} & =\left(\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\right)^{-1}\left(\mathbf{X}\mathbf{X}^{T}\right)^{-1}\mathbf{X}^{T}Y\\
\\
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E_{h} & =\sum_{i=1}^{N}\left(\mathbf{G}_{i}\mathbf{G}_{i}^{T}-\mathbf{Q}_{h}\left[\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\right]\mathbf{Q}_{h}^{T}\right)^{2}\\
\frac{\partial}{\partial\gamma_{\cdot h}}E_{h} & =-2\sum_{i=1}^{N}\left(\mathbf{G}_{i}\mathbf{G}_{i}^{T}-\mathbf{Q}_{h}\left[\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\right]\mathbf{Q}_{h}^{T}\right)\mathbf{Q}_{h}\left[\sum_{j=1}^{q}\mathbf{X}_{i,j}^{T}\right]\mathbf{Q}_{h}^{T}\\
0 & =2\sum_{i=1}^{N}\left(\mathbf{G}_{i}\mathbf{G}_{i}^{T}\mathbf{Q}_{h}\left[\sum_{j=1}^{q}\mathbf{X}_{i,j}^{T}\right]\mathbf{Q}_{h}^{T}-\mathbf{Q}_{h}\left[\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\right]\mathbf{Q}_{h}^{T}\mathbf{Q}_{h}\left[\sum_{j=1}^{q}\mathbf{X}_{i,j}^{T}\right]\mathbf{Q}_{h}^{T}\right)\\
\sum_{i=1}^{N}\left(\mathbf{G}_{i}\mathbf{G}_{i}^{T}\mathbf{Q}_{h}\left[\sum_{j=1}^{q}\mathbf{X}_{i,j}^{T}\right]\mathbf{Q}_{h}^{T}\right) & =\sum_{i=1}^{N}\left(\mathbf{Q}_{h}\left[\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\right]\mathbf{Q}_{h}^{T}\mathbf{Q}_{h}\left[\sum_{j=1}^{q}\mathbf{X}_{i,j}^{T}\right]\mathbf{Q}_{h}^{T}\right)\\
\\
\\
\\
\\
0 & =\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\mathbf{X}^{T}\left(\mathbf{G}\mathbf{G}^{T}-\mathbf{Q}_{h}\left[\mathbf{X}\gamma_{\cdot,h}\right]\mathbf{Q}_{h}^{T}\right)\\
0 & =\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\mathbf{X}^{T}\mathbf{G}\mathbf{G}^{T}-\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\mathbf{Q}_{h}\left[\mathbf{X}\gamma_{\cdot,h}\right]\mathbf{Q}_{h}^{T}\\
\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\mathbf{X}^{T}\mathbf{X}\gamma_{\cdot,h}\mathbf{Q}_{h}^{T}\mathbf{Q}_{h} & =\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\mathbf{X}^{T}\mathbf{G}\mathbf{G}^{T}\\
\mathbf{X}^{T}\mathbf{X}\gamma_{\cdot,h}\mathbf{Q}_{h}^{T}\mathbf{Q}_{h} & =\mathbf{X}^{T}\mathbf{G}\mathbf{G}^{T}\\
\gamma_{\cdot,h} & =\left(\mathbf{X}^{T}\mathbf{X}\right)^{-1}\mathbf{X}^{T}\mathbf{G}\mathbf{G}^{T}\\
\left[\mathbf{X}\gamma_{\cdot,h}\right]\mathbf{Q}_{h}^{T}\mathbf{X}^{T}\mathbf{Q}_{h}\mathbf{Q}_{h}^{T}\mathbf{Q}_{h} & =\mathbf{Q}\mathbf{X}_{i,\cdot}\mathbf{Q}^{T}\mathbf{G}\mathbf{G}^{T}
\end{align*}

\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E_{h} & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}-\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)^{2}\\
 & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\mathbf{G}_{i,j}\mathbf{G}_{i,k}-\mathbf{G}_{i,j}\mathbf{G}_{i,k}\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}-\mathbf{G}_{i,j}\mathbf{G}_{i,k}\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}+\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\\
\frac{\partial}{\partial\gamma_{\cdot h}}E_{h} & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(-2\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\mathbf{X}_{i,\cdot}+2\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\\
0 & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(-2\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\mathbf{X}_{i,\cdot}+2\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\\
0 & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(-\mathbf{G}_{i,j}\mathbf{G}_{i,k}\mathbf{X}_{i,\cdot}+\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\\
\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\mathbf{X}_{i,\cdot}\right) & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\\
\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right) & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\mathbf{X}_{i,\cdot}\right)\\
\\
\\
\\
\frac{\partial}{\partial\gamma_{\cdot h}}E_{h} & =\sum_{i=1}^{N}\sum_{j=1,k>j}^{p}-2\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}-\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\gamma_{\cdot,h}\mathbf{X}_{i,\cdot}^{T}\right)\mathbf{X}_{i,\cdot}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\\
0 & =-2\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}-\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)\mathbf{X}_{i,\cdot}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\\
\mathbf{X}_{i,\cdot}\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k} & =\mathbf{G}_{i,j}\mathbf{G}_{i,k}\\
\mathbf{X}_{i,\cdot}\gamma_{\cdot,h} & =\mathbf{G}_{i,j}\mathbf{G}_{i,k}\left(\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\right)^{-1}\\
\gamma_{\cdot,h}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}\mathbf{X}_{i,\cdot}^{T}\mathbf{X}_{i,\cdot} & =\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)\mathbf{X}_{i,\cdot}\\
\gamma_{\cdot,h} & =\left(\mathbf{G}_{i,j}\mathbf{G}_{i,k}\right)\mathbf{X}_{i,\cdot}\left(\mathbf{X}_{i,\cdot}^{T}\mathbf{X}_{i,\cdot}\right)^{-1}\mathbf{Q}_{h,j}\mathbf{Q}_{h,k}
\end{align*}

\end_inset


\end_layout

