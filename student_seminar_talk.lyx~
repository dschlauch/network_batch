\end_layout

\begin_layout Frame
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{itemize}
\end_layout

\begin_layout Plain Layout

	
\backslash
item COMBAT
\end_layout

\begin_layout Plain Layout

- Empirical Bayes approach for location/scale adjustment
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

	
\backslash
item Surrogate Variable Analysis (SVA)
\end_layout

\begin_layout Plain Layout

- SVD approach for estimating 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

	
\backslash
item Reference based (RATIO-G) methods
\end_layout

\begin_layout Plain Layout

-Scales sample measurements by the geometric mean of a group of reference
 measurements
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

	
\backslash
item Distance Weighted Discrimination (DWD)
\end_layout

\begin_layout Plain Layout

-Based on SVM
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

	
\backslash
item Many more...
\end_layout

\begin_layout Plain Layout


\backslash
end{itemize}
\end_layout

