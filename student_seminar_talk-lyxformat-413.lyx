\begin_inset Formula 
\begin{align*}
w & =\left(\frac{\sum_{i=1}^{n}\hat{\sigma}_{P,k,i}^{2}\sum_{i=1}^{n}\hat{\sigma}_{G,k,i}^{2}}{\sum_{i=1}^{n}\left[\hat{\sigma}_{P,k,i}^{2}\hat{\sigma}_{G,k,i}^{2}\right]}\right)\\
E\left[s_{k}|H_{0}\right] & =1
\end{align*}

